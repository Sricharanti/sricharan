###############################################################################
# Scenario: L_DD_ACCELEROMETER_0000_0014
# Author  : Dan Murphy
# Date    : Mon Nov 1, 2010
###############################################################################

# Begin L_DD_ACCELEROMETER_0000_0014

01  set -x; handlerSysFs.sh "get" $ACCELEROMETER_SYSFS_PATH/mode > mode.log

02  set -x; handlerSysFs.sh "set" $ACCELEROMETER_SYSFS_PATH/mode $ACCELEROMETER_MODE_MEAS400

03  set -x; handlerSysFs.sh "get" $ACCELEROMETER_SYSFS_PATH/delay > delay.log

04  set -x; handlerSysFs.sh "set" $ACCELEROMETER_SYSFS_PATH/delay "200"

05  set -x; handlerSysFs.sh "compare" $ACCELEROMETER_SYSFS_PATH/delay 200

06  set -x; handlerSysFs.sh "set" $ACCELEROMETER_SYSFS_PATH/delay "20"

07  set -x; handlerSysFs.sh "compare" $ACCELEROMETER_SYSFS_PATH/delay 20

08  set -x; handlerSysFs.sh "set" $ACCELEROMETER_SYSFS_PATH/delay "`cat delay.log`"

09  set -x; handlerSysFs.sh "compare" $ACCELEROMETER_SYSFS_PATH/delay "`cat delay.log`"

10  rm delay.log

11  set -x; handlerSysFs.sh "set" $ACCELEROMETER_SYSFS_PATH/mode "`cat mode.log`"

12  set -x; handlerSysFs.sh "compare" $ACCELEROMETER_SYSFS_PATH/mode "`cat mode.log`"

13  rm mode.log

# End L_DD_ACCELEROMETER_0000_0014
