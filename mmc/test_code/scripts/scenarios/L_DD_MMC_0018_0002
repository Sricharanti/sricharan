01_16  set -x; $UTILS_DIR_HANDLERS/handlerProcessPriority.sh "run" "dd if=/dev/urandom of=$MMCSD_DIR_TMP/file.temporal bs=1M count=50 && sync"
01_17  set -x; $UTILS_DIR_HANDLERS/handlerProcessPriority.sh "add" "1" "cp $MMCSD_DIR_TMP/file.temporal /mnt/mmc/file.temporal.1 && sync" "+20"
01_18  set -x; $UTILS_DIR_HANDLERS/handlerProcessPriority.sh "add" "2" "cp $MMCSD_DIR_TMP/file.temporal /mnt/mmc/file.temporal.2 && sync" "-20"
