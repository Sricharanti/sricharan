# Begin L_DD_MMC_SD_0017_0001

01  set -x; handlerMmcsdBlock.sh "create" "1" "ext2"

02  set -x; handlerTmpfs.sh "create" "250" $MMCSD_TMPFS_MOUNTPOINT

03  set -x; handlerCmd.sh "run" "dd if=/dev/urandom of=$MMCSD_TMPFS_MOUNTPOINT/$MMCSD_FILE_SIZE_BIG bs=1M count=20"

04  set -x; $MMCSD_DIR_HELPER/threadtest.sh $MMCSD_FILE_SIZE_BIG $MMCSD_TMPFS_MOUNTPOINT $MMCSD_MOUNTPOINT_1

05  set -x; handlerTmpfs.sh "remove" $MMCSD_TMPFS_MOUNTPOINT

06  set -x; handlerMmcsdBlock.sh "remove" "1"

# End L_DD_MMC_SD_0017_0001



