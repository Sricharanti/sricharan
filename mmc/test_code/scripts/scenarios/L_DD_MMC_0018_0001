01_09  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.07 bs=1M count=10"
01_10  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.08 bs=1M count=10"
01_11  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.09 bs=1M count=10"
01_12  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.10 bs=1M count=10"
01_13  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "execute"

01_14  set -x; $MMCSD_DIR_HELPER/handlerMmcsdBlock.sh "remove" "1" "/mnt/mmc/"

