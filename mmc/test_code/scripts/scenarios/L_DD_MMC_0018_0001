01_01  set -x; $MMCSD_DIR_HELPER/handlerMmcsdBlock.sh "create" "1" "ext2" "/mnt/mmc/"

01_02  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "clean"
01_03  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.01 bs=1M count=10"
01_04  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.02 bs=1M count=10"
01_05  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=/mnt/mmc/tmp.03 bs=1M count=10"
