# Begin L_DD_MMC_0001_0002

01  set -x; echo "Insert the MMC/SD card.."
01  set -x; sleep 10
03  set -x; ls $SYS_MMC_HOST/mmc$SLOT/ | grep mmc && exit 0 || exit 1

