# Begin CPU_AFFINITY

01  set -x; handlerCmd.sh "run" "dd if=/dev/urandom of=$MSD_TEST_FOLDER/file.temporal bs=1M count=250 && sync"
02  set -x; handlerCpuAffinity.sh "switch" "cp $MSD_TEST_FOLDER/file.temporal $MSD_TEST_FOLDER/file.cp" "10" "5"
03  set -x; handlerCpuAffinity.sh "switch" "dd if=$MSD_TEST_FOLDER/file.temporal of=$MSD_TEST_FOLDER/file.dd" "10" "5"
04  set -x; handlerCmd.sh "run" "cmp $MSD_TEST_FOLDER/file.cp $MSD_TEST_FOLDER/file.dd"

# End CPU_AFFINITY
