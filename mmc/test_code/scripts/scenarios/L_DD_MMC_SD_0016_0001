###############################################################################
# Scenario: L_DD_MMC_SD_0016_0001
# Author  : dtaylor
# Date    : updated 1Nov11
# Description: Dynamic Voltage and Frequency Scaling
###############################################################################



# Begin L_DD_MMC_SD_0016_0001

01  set -x; handlerMmcsdBlock.sh "create" "2" "dos-ext2"

02  set -x; handlerTmpfs.sh "create" "125" $MMCSD_TMPFS_MOUNTPOINT

03  set -x; handlerCmd.sh "run" "dd if=/dev/urandom of=$MMCSD_TMPFS_MOUNTPOINT/$MMCSD_FILE_SIZE_BIG bs=1M count=100"

04 set -x; $UTILS_DIR_HANDLERS/handlerCpuFreqScalFrequencies.sh run all "$MMCSD_DIR_HELPER/cpcmp.sh $MMCSD_FILE_SIZE_BIG $MMCSD_TMPFS_MOUNTPOINT $MMCSD_MOUNTPOINT_1 "

05 set -x; $UTILS_DIR_HANDLERS/handlerCpuFreqScalFrequencies.sh run all "$MMCSD_DIR_HELPER/cpcmp.sh $MMCSD_FILE_SIZE_BIG $MMCSD_TMPFS_MOUNTPOINT $MMCSD_MOUNTPOINT_2 "

06  set -x; handlerTmpfs.sh "remove" $MMCSD_TMPFS_MOUNTPOINT

07  set -x; handlerMmcsdBlock.sh "remove" "2"

# End L_DD_MMC_SD_0016_0001



