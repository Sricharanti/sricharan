# Begin L_DD_ETHERNET_0002_0002

01  set -x; handlerIfconfig.sh "get" "ipaddr" && exit 0 || exit 1

02  set -x; ping -c 5 -s 1    `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
03  set -x; ping -c 5 -s 5    `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
04  set -x; ping -c 5 -s 56   `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
05  set -x; ping -c 5 -s 560  `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
06  set -x; ping -c 5 -s 5600 `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1

# End L_DD_ETHERNET_0002_0002
