# Begin L_DD_ETHERNET_0005_0001

01  set -x; handlerIfconfig.sh "get" "ipaddr"

02  set -x; ping -c 1  `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
03  set -x; ping -c 5  `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
04  set -x; ping -c 10 `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1
05  set -x; ping -c 25 `cat $ETHERNET_EXTERNAL_HOST_IPADDR` && exit 0 || exit 1

# End L_DD_ETHERNET_0005_0001

