# Begin L_DD_ETHERNET_0001_0002

01  set -x; handlerIfconfig.sh "get" "ipaddr"

02  set -x; cat /proc/interrupts | grep $ETHERNET_IRQ_SOURCE
03  set -x; cat /proc/interrupts | grep $ETHERNET_IRQ_SOURCE | sed -e 's@:@@' | sed q | awk '{print $2}' > $ETHERNET_DIR_TMP/irq.1
04  set -x; handlerPing.sh "-c 5 `cat $ETHERNET_EXTERNAL_HOST_IPADDR`"
05  set -x; cat /proc/interrupts | grep $ETHERNET_IRQ_SOURCE | sed -e 's@:@@' | sed q | awk '{print $2}' > $ETHERNET_DIR_TMP/irq.2
06  set -x; if [ `cat $ETHERNET_DIR_TMP/irq.1` -lt `cat $ETHERNET_DIR_TMP/irq.2` ]; then exit 0; else exit 1; fi;

# End L_DD_ETHERNET_0001_0002
