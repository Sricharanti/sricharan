# Begin L_DD_ETHERNET_0001_0002

01 set -x; cat /proc/interrupts | grep eth0 | sed -e 's@:@@' | sed q | awk '{print $1}' > /tmp/IRQ1 && exit 0 || exit 1;
02 set -x; ifconfig | grep Interrupt: | sed -e 's@Interrupt:@@' | sed q | awk '{print $1}' > /tmp/IRQ2 && exit 0 || exit 1;
03 set -x; cmp /tmp/IRQ1 /tmp/IRQ2 && exit 0 || exit 1;

# End L_DD_ETHERNET_0001_0002
