# Begin L_DD_ETHERNET_0005_0002

01  set -x; handlerIfconfig.sh "get" "ipaddr"

02  set -x; ping -c 5 -s 1    `cat $ETHERNET_EXTERNAL_HOST_IPADDR`
03  set -x; ping -c 5 -s 5    `cat $ETHERNET_EXTERNAL_HOST_IPADDR`
04  set -x; ping -c 5 -s 56   `cat $ETHERNET_EXTERNAL_HOST_IPADDR`
05  set -x; ping -c 5 -s 560  `cat $ETHERNET_EXTERNAL_HOST_IPADDR`
06  set -x; ping -c 5 -s 5600 `cat $ETHERNET_EXTERNAL_HOST_IPADDR`

# End L_DD_ETHERNET_0005_0002

