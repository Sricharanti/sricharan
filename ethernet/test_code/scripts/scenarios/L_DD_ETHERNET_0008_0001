01  set -x; ifconfig | grep eth0 && ifconfig | grep addr: | sed -e 's@inet addr:@@' | sed q | awk '{print $1}' > $ETHERNET_DIR_TMP/ipaddr && exit 0 || exit 1;

02  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "clean"
03  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
