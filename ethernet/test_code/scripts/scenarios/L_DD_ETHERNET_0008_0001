07  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
08  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
09  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
11  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
12  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
13  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`"
14  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "execute"

