# Begin L_DD_ETHERNET_0003_0001

# Valid values
01  set -x; handlerIfconfig.sh "set" "mtu" 500  && exit 0 || exit 1
02  set -x; handlerIfconfig.sh "set" "mtu" 1000 && exit 0 || exit 1
03  set -x; handlerIfconfig.sh "set" "mtu" 1500 && exit 0 || exit 1

# Range of valid values
04  set -x; for i in `seq 500 1500`; do handlerIfconfig.sh "set" "mtu" $i; done && exit 0 || exit 1

# Default Value
05  set -x; handlerIfconfig.sh "set" "mtu" 1500 && exit 0 || exit 1

# End L_DD_ETHERNET_0003_0001

