# Begin L_DD_ETHERNET_0014_0001

01  set -x; handlerIfconfig.sh "get" "ipaddr"
02  set -x; handlerNfs.sh "verify"
#03  set -x; handlerNuttcp.sh "server" "start"

# Suspend/Resume
04  set -x; handlerSuspendResume.sh "run" "5" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`"
05  set -x; handlerSuspendResume.sh "run" "5" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.01 bs=1M count=100"
06  set -x; handlerSuspendResume.sh "run" "5" 'handlerNuttcp.sh "remote" "common" "-w6m -P6000"'

# CPU1 Hotplug
07  set -x; handlerCpuHotPlug.sh "run" "5" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`"
08  set -x; handlerCpuHotPlug.sh "run" "5" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.01 bs=1M count=100"
09  set -x; handlerCpuHotPlug.sh "run" "5" 'handlerNuttcp.sh "remote" "common" "-w6m -P6000"'

# Retention
10  set -x; handlerPowerRetention.sh  enable

11  set -x; handlerCmd.sh "run" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`"
12  set -x; handlerPowerRetention.sh  check  "l3init_pwrdm"
13  set -x; handlerCmd.sh "run" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.01 bs=1M count=100"
14  set -x; handlerPowerRetention.sh  check  "l3init_pwrdm"
15  set -x; handlerNuttcp.sh "remote" "common" "-w6m -P6000"
16  set -x; handlerPowerRetention.sh  check  "l3init_pwrdm"

17  set -x; handlerPowerRetention.sh  disable

# Open Switch Retention
18  set -x; handlerCmd.sh "run" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`"
19  set -x; handlerPowerOSWR.sh  check  "l3init_pwrdm"
20  set -x; handlerCmd.sh "run" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.01 bs=1M count=100"
21  set -x; handlerPowerOSWR.sh  check  "l3init_pwrdm"
22  set -x; handlerNuttcp.sh "remote" "common" "-w6m -P6000"
23  set -x; handlerPowerOSWR.sh  check  "l3init_pwrdm"

#24  set -x; handlerNuttcp.sh "server" "stop"

# End L_DD_ETHERNET_0014_0001
