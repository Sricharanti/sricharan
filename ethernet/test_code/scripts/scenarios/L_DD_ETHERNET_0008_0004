# Begin L_DD_ETHERNET_0008_0004

01  set -x; ifconfig | grep eth0 && ifconfig | grep addr: | sed -e 's@inet addr:@@' | sed q | awk '{print $1}' > $ETHERNET_DIR_TMP/ipaddr && exit 0 || exit 1;

02  set -x; $UTILS_DIR_HANDLERS/handlerIrqAffinity.sh "switch" "ping -c 100 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_DIR_TMP/ipaddr`" "5" "15" "194" && exit 0 || exit 1

# End L_DD_ETHERNET_0008_0004