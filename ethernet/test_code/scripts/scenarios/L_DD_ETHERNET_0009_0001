13  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "5"
14  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "10"
15  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "15"
16  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "20"

17  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.01 bs=1M count=5" "-20"
18  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.02 bs=1M count=5" "-15"
19  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.03 bs=1M count=5" "-10"
20  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.04 bs=1M count=5" "-5"
21  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.05 bs=1M count=5" "0"
22  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.06 bs=1M count=5" "5"
23  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.07 bs=1M count=5" "10"
24  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.08 bs=1M count=5" "15"
25  set -x; handlerProcessPriority.sh "assign" "dd if=$ETHERNET_DEV_URANDOM of=`cat $ETHERNET_NFS_DIR`/eth.09 bs=1M count=5" "20"

26  set -x; handlerProcessPriority.sh "execute"

