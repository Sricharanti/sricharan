# Begin L_DD_ETHERNET_0009_0001

01  set -x; handlerIfconfig.sh "get" "ipaddr"
02  set -x; handlerNfs.sh "verify"

# Process Priority 2 Commands

03  set -x; handlerProcessPriority.sh "add" "1" "ping -c 15 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "-20"
04  set -x; handlerProcessPriority.sh "add" "2" "ping -c 15 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "+20"
05  set -x; handlerProcessPriority.sh "execute" "1"
06  set -x; handlerProcessPriority.sh "verify"

# Process Priority N Commands No Delay

07  set -x; handlerProcessPriority.sh "clean"

08  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "-20"
09  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "-15"
10  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "-10"
11  set -x; handlerProcessPriority.sh "assign" "ping -c 25 -s $ETHERNET_PACKET_SIZE `cat $ETHERNET_EXTERNAL_HOST_IPADDR`" "-5"
