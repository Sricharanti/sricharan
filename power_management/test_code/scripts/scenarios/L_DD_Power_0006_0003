# Begin  L_DD_Power_0006_0003

0001  insmod $TESTMODS/co_test.ko co_type=0x3 target_val=250 && sleep 1 && cat /proc/cpuinfo | grep "BogoMIPS"  && sleep 1 && echo "VALIDATION CRITERIA: Verify that \"BogoMIPS is near to\" 250.00 " && sleep 1 && $UTILBIN/akey && rmmod co_test.ko && echo "Reboot the system after this test" && sleep 1 && $UTILBIN/waitkey && exit 0 || exit 1

# End L_DD_Power_0006_0003
