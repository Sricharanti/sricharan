# Begin  L_DD_Power_0006_001a

0001_1	insmod $TESTMODS/co_test.ko co_type=0x1 target_val=1 && sleep 1 && cat /proc/cpuinfo | grep "BogoMIPS"  && sleep 1 && echo "VALIDATION CRITERIA: Verify that \"BogoMIPS is near to\" 125.00 " && sleep 1 && $UTILBIN/akey && rmmod co_test.ko && echo "VALIDATION CRITERIA: Verify that \"Pre Notifier function called for level 1\" message is printed " && sleep 1 && $UTILBIN/akey && sleep 2 && echo "Reboot the system after this test" && sleep 1 && $UTILBIN/waitkey && exit 0 || exit 1

# End L_DD_Power_0006_001a
