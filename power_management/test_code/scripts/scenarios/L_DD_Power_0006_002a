# Begin  L_DD_Power_0006_002a

0001	insmod $TESTMODS/co_test.ko co_type=0x2 target_val=0x2 && sleep 1 && cat /sys/power/vdd2_opp_value > $TMPBASE/vdd2.tmp && cat /$TMPBASE/vdd2.tmp | grep "2"  && rmmod co_test.ko && sleep 2 && echo "Reboot the system after this test" && $UTILBIN/waitkey && exit 0 || exit 1

# End L_DD_Power_0006_002a
