# Begin L_DD_TIMER_0010_0001

0001 set -x; handlerIrqAffinity.sh "switch" 'sleep 20 & timeout=0; iterations=50 ; while [ $timeout -lt $iterations ]; do for i in  1  2  3  4  5  6  7  8  9  a  b  c  d  e  f; do $TESTSCRIPT/requestTimerNumber.sh 2 && ($TESTSCRIPT/requestTimerNumber.sh 10 && continue || exit 1;) || exit 1; done; timeout=$((timeout=$timeout+1)); done' "10" "3" "69"

# End L_DD_TIMER_0010_0001
