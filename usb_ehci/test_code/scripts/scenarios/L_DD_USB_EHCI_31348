################################################################################
# Scenario:    L_DD_USB_EHCI_31348
# Author  :    Andrii Danylov
# Date    :    April 10th, 2013
# Description: SMP:  Process ParallelismDWC_Host
# Host USB DWC_host driver shall support process parallelism.
################################################################################

# Begin L_DD_USB_EHCI_31348

01  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "clean"
02  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.01 bs=1M count=10"
03  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.02 bs=1M count=10"
04  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.03 bs=1M count=10"
05  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.04 bs=1M count=10"
06  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.05 bs=1M count=10"
07  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.06 bs=1M count=10"
08  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.07 bs=1M count=10"
09  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.08 bs=1M count=10"
10  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.09 bs=1M count=10"
11  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "dd if=/dev/urandom of=$USBHOST_TEST_FOLDER/tmp.10 bs=1M count=10"
12  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "execute"

13  set -x; rm $USBHOST_TEST_FOLDER/tmp.*

# End L_DD_USB_EHCI_31348
