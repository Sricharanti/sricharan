# Begin L_DD_GPIO_0006_0001

0001 set -x; $UTILS_DIR_SCRIPTS/system_message_buffer.sh "system.log" && exit 0 || exit 1
0002 set -x; cat "system.log" | grep -w "omap_hwmod: gpio" | grep "softreset failed" && exit 1 || exit 0
0003 set -x; rm "system.log" && exit 0 || exit 1

# End L_DD_GPIO_0006_0001
