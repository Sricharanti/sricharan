	$(CC) $(CFLAGS) -DVERSION=\"$(ver)\" -o ${ionice} ionice.c
taskset: taskset.c
	$(CC) $(CFLAGS) -DVERSION=\"$(ver)\" -o ${taskset} taskset.c

