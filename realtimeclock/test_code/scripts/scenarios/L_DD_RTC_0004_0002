01  set -x; echo -e "$APP_VAL_INPUT_USER\n $APP_VALID_DATE\n $APP_VALID_TIME\n" | $APP_TIME_SET $DEVFS_RTC && $APP_TIME_GET $DEVFS_RTC | grep "${MSG_OK_DATE_TIME_DEFAULT} ${RTC_VALID_DATE}" && exit 0 || exit 1
02	 set -x; echo -e "$ALARM_SEC\n $ALARM_VALUE\n" | $APP_ALARM_SET $DEVFS_RTC && exit 0 || exit 1

