01		set -x; echo -e "$APP_VAL_INPUT_DEFAULT\n" | $APP_TIME_SET $DEVFS_RTC | grep "${MSG_DATE_TIME_SET_NEW} ${DEF_RTC_VALID_DATE} ${DEF_RTC_VALID_TIME}" && cat $PROCFS_RTC | grep "rtc_time" | grep "${DEF_PROC_VALID_TIME_PROCFS}" && cat $PROCFS_RTC | grep "rtc_date" | grep "${DEF_PROC_VALID_DATE}" && exit 0 || exit 1

