01  set -x; $HELPER_KERNEL_MESSAGES "erase"
02  set -x; insmod $MODULE_HELLO_WORLD
03  set -x; $HELPER_KERNEL_MESSAGES "search" "Hello World!"
04  set -x; $HELPER_KERNEL_MESSAGES "search" "Bye Hello World!"

# End L_DD_DEMO_0002_0001
