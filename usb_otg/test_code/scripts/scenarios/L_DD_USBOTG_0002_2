foo	echo 'L_DD_USBOTG_0002_2'

0  insmod $MODULE_PATH/g_zero.ko  &&  echo -e '\n\n\t Testing OMAP as a USB-OTG-Device \n\n'  &&  sleep $DELAY1  &&  exit 0 || exit 1

1  export ENUM_INFO="Zero"; ${TESTROOT}/test_runner.sh -p L_DD_USBOTG_0002_2_core  && exit 0 || exit 1

2  rmmod g_zero  &&  exit 0 || exit 1

bar	echo 'L_DD_USBOTG_0002_2'
