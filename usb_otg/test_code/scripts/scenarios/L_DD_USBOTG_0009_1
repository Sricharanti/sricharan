foo	echo 'L_DD_USBOTG_0009_1'

0  insmod $MODULE_PATH/g_zero.ko  &&  echo -e '\n\n\t Testing the Suspend and Resume functionality of the USB OTG controller. \n\t Cable switching after a suspend and resume cycle. \n\n'  &&  sleep $DELAY1  &&  exit 0 || exit 1

1   ${TESTROOT}/test_runner.sh -p L_DD_USBOTG_0009_1_core  &&  exit 0 || exit 1

2  rmmod g_zero && exit 0 || exit 1

bar	echo 'L_DD_USBOTG_0009_1'
