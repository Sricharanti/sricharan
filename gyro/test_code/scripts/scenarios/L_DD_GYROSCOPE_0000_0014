06 handlerSysFs.sh "set" $GYRO_DELAY 60

07 handlerSysFs.sh "compare" $GYRO_DELAY 60

08 handlerSysFs.sh "set" $GYRO_DELAY 200

09 handlerSysFs.sh "compare" $GYRO_DELAY 200

10 handlerSysFs.sh "set" $GYRO_DELAY 1

