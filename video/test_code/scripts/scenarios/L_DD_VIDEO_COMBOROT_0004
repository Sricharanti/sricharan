COMMENT comment.sh "Rotation and Scaling Combo Testcases"
COMMENT comment.sh "***********Width variable and height fixed for UYVY************"
COMMENT comment.sh "***********Scaling with No Rotation for UYVY************"

0000_SET_VD1 set -x;overlay_switch.sh 0 1                    && exit 0 || exit 1
0000_SET_VD2 set -x;overlay_switch.sh 0 2                    && exit 0 || exit 1

COMMENT comment.sh "UYVY-480:480"
0001_YUYV_VD1 set -x; setupimage.sh 1 "YUYV 1280 720"				&& exit 0 || exit 1
0001_UYVY_VD2 set -x; setupimage.sh 2 "UYVY 480 480"				&& exit 0 || exit 1
0001_UYVY_VD4 set -x; setupimage.sh 4 "UYVY 480 480"				&& exit 0 || exit 1
0001_VD1 set -x; wbstream.sh 1 $VIDEOFILES/720p.yuv 2

