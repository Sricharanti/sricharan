foo   echo "L_DD_VID_0012"

0001A set -x; video1pm.sh "0012_0001_A"  "$RESUME $STATE1 $STATE2 $SUSPEND $RESUME" "1 YUYV 176 144" "1 video_qcif_yuv_75 1" "Test suspend/resume for video-1..." && exit 0 || exit 1
0001B set -x; video1pm.sh "0012_0001_B" "DPM 5 TIMES" "1 YUYV 176 144" "1 video_qcif_yuv_75 1" "Test DPM-Freq-Scaling 5 times for video-1..."  && exit 0 || exit 1
0001C set -x; video1pm.sh "0012_0001_C" "SLEEP 5 TIMES" "1 YUYV 176 144" "1 video_qcif_yuv_75 1" "Test DPM-Sleep 5 times for video-1..."  && exit 0 || exit 1

