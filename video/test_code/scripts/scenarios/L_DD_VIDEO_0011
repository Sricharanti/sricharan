COMMENT comment.sh "Verify the background color of the canvas can be configured"
#0001 $TESTBIN/fbtest -b | grep "fault" && exit 1 || exit 0 
COMMENT comment.sh "SENDING GFX PIPELINE TO TV"
0001 echo tv > /sys/class/display_control/omap_disp_control/graphics
0001 set -x; background.sh "1" "123456"&& exit 1 || exit 0
0001 set -x; background.sh "1" "765432"&& exit 1 || exit 0
COMMENT_INT comment.sh "SENDING GFX PIPELINE TO TV"
0001 echo lcd > /sys/class/display_control/omap_disp_control/graphics
#0001 $TESTBIN/fbtest -u | grep "fault" && exit 1 || exit 0 

# End "L_DD_VID_0011"
