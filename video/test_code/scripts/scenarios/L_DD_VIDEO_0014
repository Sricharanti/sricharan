COMMENT comment.sh "Verify that the video rendered can be up/down scaled in horizontal/vertical direction using HW decimation in all 3 video pipelines"

0001_VD1_SAN set -x; decimation.sh 1 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 864 480" && exit 0 || exit 1
0001_VD2_SAN set -x; decimation.sh 2 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 864 480" && exit 0 || exit 1
0001_VD3_SAN set -x; decimation.sh 3 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 864 480" && exit 0 || exit 1
0002_VD1_SAN set -x; decimation.sh 1 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 432 480" && exit 0 || exit 1
0002_VD2_SAN set -x; decimation.sh 2 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 432 480" && exit 0 || exit 1
0002_VD3_SAN set -x; decimation.sh 3 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 432 480" && exit 0 || exit 1
0003_VD1 set -x; decimation.sh 1 160 120 "RGB565" $VIDEOFILES/video_qqvga_rgb_30 "0 0 432 60" && exit 0 || exit 1
0003_VD2 set -x; decimation.sh 2 160 120 "RGB565" $VIDEOFILES/video_qqvga_rgb_30 "0 0 432 60" && exit 0 || exit 1
0003_VD3 set -x; decimation.sh 3 160 120 "RGB565" $VIDEOFILES/video_qqvga_rgb_30 "0 0 432 60" && exit 0 || exit 1
0004_VD1_SAN set -x; decimation.sh 1 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 1728 480" && exit 0 || exit 1
0004_VD2_SAN set -x; decimation.sh 2 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 1728 480" && exit 0 || exit 1
0004_VD3_SAN set -x; decimation.sh 3 640 480 "YUYV"   $VIDEOFILES/test.yuv "0 0 1728 480" && exit 0 || exit 1
