COMMENT comment.sh "Verify different output destructive resolution formats rendered on video 1 2 and 3 pipelines on primary"

0001_VD1_SAN1 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30 "" "0 0 160 120"		&& exit 0 || exit 1
0001_VD1_SAN2 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30 "" "0 0 -160 -120"		&& exit 0 || exit 1
0001_VD1_SAN3 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30	"" "0 0 x y"			&& exit 0 || exit 1
0001_VD1_SAN4 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30	"" "0 0 2050 2050"		&& exit 0 || exit 1
0001_VD1_SAN5 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30	"" "0 0 1 1"			&& exit 0 || exit 1
0001_VD1_SAN6 set -x; imgstream.sh 1 "RGB565 160 120" $VIDEOFILES/video_qqvga_rgb_30	"" "0 0 5000 5000"		&& exit 0 || exit 1

