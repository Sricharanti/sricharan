COMMENT comment.sh "Rotation and Scaling Combo Testcases"
COMMENT comment.sh "***********Width fixed and height variable for YUYV ************"
COMMENT comment.sh "***********Scaling with No Rotation for YUYV ************"

0000_SET_VD2 set -x;overlay_switch.sh 0 1                    && exit 0 || exit 1
0000_SET_VD2 set -x;overlay_switch.sh 0 2                    && exit 0 || exit 1

COMMENT comment.sh "YUYV-480:480"
0011_VD1_STRM1 set -x; imgstream.sh 1 "YUYV 864 480" $VIDEOFILES/wvga_video.yuv "0 0 864 480" "0 0 480 480" "2" "1"		&& exit 0 || exit 1

