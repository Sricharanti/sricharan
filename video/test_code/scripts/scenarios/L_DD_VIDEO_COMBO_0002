COMMENT comment.sh "YUYV-Mirror:0"
0025_VD1_STRM1 set -x; mirrorstream.sh 1 0 $VIDEOFILES/720p.yuv "2"  && exit 0 || exit 1

COMMENT comment.sh "************Changing Position*************"

