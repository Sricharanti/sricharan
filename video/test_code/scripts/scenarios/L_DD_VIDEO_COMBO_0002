# Begin "L_DD_VIDEO_COMBO_0002"

COMMENT comment.sh "Rotation - Mirroring Combo Testcase for YUYV on LCD1"
 
0000_SET_VD1 set -x;overlay_switch.sh 0 1                    && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:0"
0011_VD1_STRM1 set -x; rotation_destruct.sh 1 "YUYV 1280 720" $VIDEOFILES/720p.yuv "0 0 320 320" "0"  && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:90"
0012_VD1_STRM1 set -x; rotation_destruct.sh 1 "YUYV 1280 720" $VIDEOFILES/720p.yuv "0 0 320 320" "90"  && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:180"
0013_VD1_STRM1 set -x; rotation_destruct.sh 1 "YUYV 1280 720" $VIDEOFILES/720p.yuv "0 0 320 320" "180"  && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:270"
0014_VD1_STRM1 set -x; rotation_destruct.sh 1 "YUYV 1280 720" $VIDEOFILES/720p.yuv "0 0 320 320" "270"  && exit 0 || exit 1


COMMENT comment.sh "YUYV-Mirror:1"
0020_VD1_STRM1 set -x; mirrorstream.sh 1 1 $VIDEOFILES/720p.yuv "2"  && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:0"
0021_VD1_STRM1 set -x; rotation_destruct.sh 1 "YUYV 1280 720" $VIDEOFILES/720p.yuv "0 0 320 320" "0"  && exit 0 || exit 1

COMMENT comment.sh "YUYV-Rotation:90"
