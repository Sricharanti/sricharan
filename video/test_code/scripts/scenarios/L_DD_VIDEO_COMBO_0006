# Begin "L_DD_VIDEO_COMBO_0006"

COMMENT comment.sh "Rotation - Mirroring Combo Testcase for NV12 on LCD2"
 
0000_SET_VD1 set -x;overlay_switch.sh 1 1                    && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:0"
0011_VD1_STRM1 set -x; rotation_destruct.sh 1 "NV12 352 288" $VIDEOFILES/ice_352x288_10frames.nv12 "0 0 320 320" "0"  && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:90"
0012_VD1_STRM1 set -x; rotation_destruct.sh 1 "NV12 352 288" $VIDEOFILES/ice_352x288_10frames.nv12 "0 0 320 320" "90"  && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:180"
0013_VD1_STRM1 set -x; rotation_destruct.sh 1 "NV12 352 288" $VIDEOFILES/ice_352x288_10frames.nv12 "0 0 320 320" "180"  && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:270"
0014_VD1_STRM1 set -x; rotation_destruct.sh 1 "NV12 352 288" $VIDEOFILES/ice_352x288_10frames.nv12 "0 0 320 320" "270"  && exit 0 || exit 1


COMMENT comment.sh "NV12-Mirror:1"
0020_VD1_STRM1 set -x; mirrorstream.sh 1 1 $VIDEOFILES/ice_352x288_10frames.nv12 "2"  && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:0"
0021_VD1_STRM1 set -x; rotation_destruct.sh 1 "NV12 352 288" $VIDEOFILES/ice_352x288_10frames.nv12 "0 0 320 320" "0"  && exit 0 || exit 1

COMMENT comment.sh "NV12-Rotation:90"
