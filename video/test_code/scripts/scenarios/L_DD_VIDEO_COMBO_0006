COMMENT comment.sh "NV12-Mirror:0"
0025_VD1_STRM1 set -x; mirrorstream.sh 1 0 $VIDEOFILES/ice_352x288_10frames.nv12 "2"  && exit 0 || exit 1

COMMENT comment.sh "************Changing Position*************"

COMMENT comment.sh "NV12-Rotation:0"
