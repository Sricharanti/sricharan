COMMENT comment.sh "Rotation and Scaling Combo Testcases"
COMMENT comment.sh "***********Width variable and height fixed for RGB32************"
COMMENT comment.sh "***********Scaling with No Rotation for RGB32************"

0000_SET_VD1 set -x;overlay_switch.sh 0 1                    && exit 0 || exit 1
0000_SET_VD2 set -x;overlay_switch.sh 0 2                    && exit 0 || exit 1

COMMENT comment.sh "RGB32-480:480"
0001_NV12_VD1 set -x; setupimage.sh 1 "NV12 1920 1080"				&& exit 0 || exit 1
0001_RGB32_VD2 set -x; setupimage.sh 2 "RGB32 480 480"				&& exit 0 || exit 1
0001_RGB32_VD4 set -x; setupimage.sh 4 "RGB32 480 480"				&& exit 0 || exit 1
0001_VD1 set -x; wbstream.sh 1 $VIDEOFILES/1080p.NV12 2

