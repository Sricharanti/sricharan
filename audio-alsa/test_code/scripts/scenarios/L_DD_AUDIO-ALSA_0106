0000_1	set -x; $TESTSCRIPT/CheckPreconditions.sh $SAMPLESDIR
0000_2	set -x; $TESTSCRIPT/OMAP4ClearPaths.sh "Playback" && $TESTSCRIPT/OMAP4ClearPaths.sh "Capture" && exit 0 || exit 1

0001_1	set -x; handlerProcessParallelism.sh "clean"
0001_2	set -x; handlerProcessParallelism.sh "add" "$TESTSCRIPT/OMAP4Playback.sh Multimedia Handsfree plughw:0,0 $SAMPLESDIR/Sample-S16_LE-48000-2-Long.wav 30"
0001_3	set -x; handlerProcessParallelism.sh "add" "handlerCmd.sh run \"sleep 5; amixer cset name='Handsfree Playback Volume' 0; sleep 5; amixer cset name='Handsfree Playback Volume' 13\""
0001_4  set -x; handlerProcessParallelism.sh "execute"

0001_5	set -x; handlerProcessParallelism.sh "clean"
0001_6	set -x; handlerProcessParallelism.sh "add" "$TESTSCRIPT/OMAP4Playback.sh Multimedia Headset plughw:0,0 $SAMPLESDIR/Sample-S16_LE-48000-2-Long.wav 30"
