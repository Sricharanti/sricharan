0002_3	set -x; handlerProcessParallelism.sh "add" "handlerCmd.sh run \"sleep 5; amixer cset name='Capture Volume' 0; sleep 5; amixer cset name='Capture Volume' 4; sleep 5; amixer cset name='AMIC UL Volume' 0; sleep 5; amixer cset name='AMIC UL Volume' 120\""
