# Begin L_DD_AUDIO-ALSA_0130

0000_1	set -x; $TESTSCRIPT/CheckPreconditions.sh $SAMPLESDIR
0000_2	set -x; $TESTSCRIPT/OMAP4ClearPaths.sh "Playback" && exit 0 || exit 1

0002_1	set -x; $TESTSCRIPT/OMAP4TinyStress.sh Multimedia Headset "0"  "$SAMPLESDIR/Sample-S16_LE-48000-2-Long.wav" 100 && $TESTSCRIPT/OMAP4ClearPaths.sh "Playback" && exit 0  exit 0 || exit 1
0002_2	set -x; $TESTSCRIPT/OMAP4TinyStress.sh Tones Headset "3"  "$SAMPLESDIR/Sample-S16_LE-48000-2-Long.wav" 100 && $TESTSCRIPT/OMAP4ClearPaths.sh "Playback" && exit 0 exit 0 || exit 1
0002_2	set -x; $TESTSCRIPT/OMAP4TinyStress.sh Voice Headset "2"  "$SAMPLESDIR/Sample-S16_LE-16000-2.wav" 100  && exit 0 exit 0 || exit 1

0003_1	set -x; $TESTSCRIPT/OMAP4ClearPaths.sh "Playback" && exit 0 || exit 1

# End L_DD_AUDIO-ALSA_0106
