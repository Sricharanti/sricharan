# Begin L_DD_LED_0000_0004

echo 1 > $LED_SYSFS_PATH/omap4\:red\:user/brightness;echo "\n\n\n\nCheck RED User LED is ON\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;
echo 0 > $LED_SYSFS_PATH/omap4\:red\:user/brightness;echo "\n\n\n\nCheck RED USER LED is OFF\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;

echo 1 > $LED_SYSFS_PATH/omap4\:green\:user/brightness;echo "\n\n\n\nCheck GREEN User LED is ON\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;
echo 0 > $LED_SYSFS_PATH/omap4\:green\:user/brightness;echo "\n\n\n\nCheck GREEN USER LED is OFF\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;

echo 1 > $LED_SYSFS_PATH/omap4\:blue\:user/brightness;echo "\n\n\n\nCheck BLUE User LED is ON\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;
echo 0 > $LED_SYSFS_PATH/omap4\:blue\:user/brightness;echo "\n\n\n\nCheck BLUE USER LED is OFF\n"; sleep 1; $UTILS_DIR_BIN/akey $DEVFS_KEYPAD;

# End L_DD_LED_0000_0004
