# Begin L_DD_ONENAND_0004_0002

# Device 1
01  set -x; $TESTBIN/flash_eraseall $MTD_CHAR_DEV1 && exit 0 || exit 1
02  set -x; dd if=$JFFS2_IMAGE of=$MTD_BLK_DEV1  && exit 0 || exit 1
03  set -x; dd if=$MTD_BLK_DEV1 of=$TEMP_FILE && exit 0 || exit 1
04  set -x; sync && rm $TEMP_FILE && exit 0 || exit 1

# Device 2
05  set -x; $TESTBIN/flash_eraseall $MTD_CHAR_DEV2 && exit 0 || exit 1
06  set -x; dd if=$JFFS2_IMAGE of=$MTD_BLK_DEV2  && exit 0 || exit 1
07  set -x; dd if=$MTD_BLK_DEV2 of=$TEMP_FILE && exit 0 || exit 1
08  set -x; sync && rm $TEMP_FILE && exit 0 || exit 1

# End L_DD_ONENAND_0004_0002
