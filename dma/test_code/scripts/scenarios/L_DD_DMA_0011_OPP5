# Begin L_DD_DMA_0011_OPP5
0001	set -x; opp5.sh
0002	set -x; echo Running on OPP5
0003	set -x; insmod $TESTMODS/dma_single_channel.ko
0004	set -x; insmod $TESTMODS/dma_priority_low.ko
0005	set -x; cat /proc/driver/dma_priority_low | grep PASS
0006	set -x; rmmod dma_priority_low
0007	set -x; rmmod dma_single_channel
# End L_DD_DMA_0011_OPP5
