0001    set -x; insmod $TESTMODS/dma_chain_transfer.ko
0002    set -x; insmod $TESTMODS/dma_chain_static_invalid.ko
0003    set -x; cat /proc/driver/dma_chain_static | grep PASS && exit 1 || exit 0;
0004    set -x; rmmod dma_chain_static_invalid
0005    set -x; rmmod dma_chain_transfer
