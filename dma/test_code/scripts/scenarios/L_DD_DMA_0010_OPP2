# Begin L_DD_DMA_0010_OPP2
0001	set -x; opp2.sh
0002	set -x; echo Running on OPP2
0003	set -x; insmod $TESTMODS/dma_single_channel.ko
0004	set -x; insmod $TESTMODS/dma_priority_high.ko
0005	set -x; cat /proc/driver/dma_priority_high | grep PASS
0006	set -x; rmmod dma_priority_high
0007	set -x; rmmod dma_single_channel
# End L_DD_DMA_0010_OPP2
