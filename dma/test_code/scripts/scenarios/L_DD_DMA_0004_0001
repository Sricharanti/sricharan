# Begin L_DD_DMA_0004

0001	set -x; insmod $TESTMODS/dma_single_channel.ko
0002	set -x; insmod $TESTMODS/dma_addrmode_singleidx.ko
0004	set -x; rmmod dma_addrmode_singleidx
0003  set -x; cat /proc/driver/dma_addrmode_singleidx | grep PASS
0005	set -x; rmmod dma_single_channel

# End L_DD_DMA_0004
