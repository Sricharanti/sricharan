# Begin L_DD_DMA_0020_OPP2
0001	set -x; cd /sys/devices/system/cpu/cpu0/cpufreq/
0002	set -x; pwd
0003	set -x; opp2.sh
0004	set -x; echo Running on OPP2
0005	set -x; insmod $TESTMODS/dma_single_channel.ko
0006	set -x; insmod $TESTMODS/dma_static_self_link.ko
0007	set -x; cat /proc/driver/dma_static_self_link | grep PASS
0008	set -x; rmmod dma_static_self_link
0009	set -x; rmmod dma_single_channel
# End L_DD_DMA_0020_OPP2
