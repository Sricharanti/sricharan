0001	set -x; insmod $TESTMODS/dma_single_channel.ko
0002	set -x; insmod $TESTMODS/dma_endianism_big.ko
0003  set -x; cat /proc/driver/dma_endianism_big | grep PASS
0004	set -x; rmmod dma_endianism_big
0005	set -x; rmmod dma_single_channel

# End L_DD_DMA_0009