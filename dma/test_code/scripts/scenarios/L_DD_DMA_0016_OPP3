# Begin L_DD_DMA_0016_OPP3
0001	set -x; opp3.sh
0002	set -x; echo Running on OPP3
0003	set -x; insmod $TESTMODS/dma_single_channel.ko
0004	set -x; insmod $TESTMODS/dma_thread_access.ko
0005    set -x; cat /proc/driver/dma_thread_access | grep PASS
0006	set -x; rmmod dma_thread_access
0007	set -x; rmmod dma_single_channel
# End L_DD_DMA_0016_OPP3
