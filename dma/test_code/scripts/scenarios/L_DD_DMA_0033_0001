# Begin L_DD_DMA_0033_0001

0001	set -x; insmod $MODDIR/dma_single_channel.ko
0002	set -x; insmod $MODDIR/dma_unlink_before_stop_channels.ko
0003	set -x; rmmod dma_unlink_before_stop_channels
0004	set -x; cat /proc/driver/dma_test_status | grep PASSED
0005	set -x; rmmod dma_single_channel

# End L_DD_DMA_0033_0001
