# Begin L_DD_DMA_0005_OPP1
0001	set -x; opp1.sh
0002	set -x; echo Running on OPP1
0003	set -x; insmod $TESTMODS/dma_single_channel.ko
0004	set -x; insmod $TESTMODS/dma_burst_disabled.ko
0005	set -x; cat /proc/driver/dma_burst_disabled | grep PASS
0006	set -x; rmmod dma_burst_disabled
0007	set -x; rmmod dma_single_channel
# End L_DD_DMA_0005_OPP1
