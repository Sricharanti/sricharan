# Begin L_DD_DMA_0013

0001	set -x; insmod $TESTMODS/dma_single_channel.ko
0002	set -x; insmod $TESTMODS/dma_transfer_query.ko
0003  set -x; cat /proc/driver/dma_transfer_query | grep PASS
0004	set -x; rmmod dma_transfer_query
0005	set -x; rmmod dma_single_channel

# End L_DD_DMA_0013