# Begin L_DD_DMA_0014_OPP1
0001	set -x; opp1.sh
0002	set -x; echo Running on OPP1
0003	set -x; insmod $TESTMODS/dma_chain_transfer.ko
0004	set -x; insmod $TESTMODS/dma_chain_dynamic.ko
0005	set -x; cat /proc/driver/dma_chain_dynamic | grep PASS
0006	set -x; rmmod dma_chain_dynamic
0007	set -x; rmmod dma_chain_transfer
# End L_DD_DMA_0014_OPP1
