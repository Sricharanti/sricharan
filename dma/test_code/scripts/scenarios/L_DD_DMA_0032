# Begin L_DD_DMA_0032
0001 set -x; echo DMA Dynamic Self linking
0002 set -x; insmod $TESTMODS/dma_single_channel.ko
0003 set -x; insmod $TESTMODS/dma_dynamic_self_link.ko
0004 set -x; cat /proc/driver/dma_self_link | grep PASS
0005 set -x; rmmod dma_dynamic_self_link
0006 set -x; rmmod dma_single_channel
# Begin L_DD_DMA_0032

