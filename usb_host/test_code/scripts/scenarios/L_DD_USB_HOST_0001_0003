###############################################################################
# Scenario: L_DD_USB_HOST_0001_0003
# Testing : Test if the OHCI_Host can use USB Mass Storage Devices
###############################################################################

# Begin L_DD_USB_HOST_0001_0003
01  set -x; handlerUsbHost.sh  "create"  ohci "/mnt/mass_storage"
02  set -x; mount && sleep 10
03  set -x; handlerUsbHost.sh  "remove"  ohci "/mnt/mass_storage"
# End L_DD_USB_HOST_0001_0003
