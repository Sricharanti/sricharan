# Begin L_DD_USB_HOST_0001_0001
01  set -x; handlerUsbHost.sh  "create"  mentor "/mnt/mass_storage"
02  set -x; mount && sleep 10 
03  set -x; handlerUsbHost.sh  "remove"  mentor "/mnt/mass_storage"
# End L_DD_USB_HOST_0001_0001
