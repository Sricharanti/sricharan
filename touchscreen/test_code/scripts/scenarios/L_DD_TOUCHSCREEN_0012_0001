11  set -x; handlerProcessPriority.sh "assign" "dd if=/dev/urandom of=/mnt/tmpfs/file.temporal bs=1M count=5 && sync"  "20"

12  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10" "-20"
