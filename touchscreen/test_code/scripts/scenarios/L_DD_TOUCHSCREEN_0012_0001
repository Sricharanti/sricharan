16  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10"   "0"
17  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10"   "5"
18  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10"  "10"
19  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10"  "15"
20  set -x; handlerProcessPriority.sh "assign" "handlerEvtest.sh run $TOUCHSCREEN_DEVFS_PRIMARY 10"  "20"

