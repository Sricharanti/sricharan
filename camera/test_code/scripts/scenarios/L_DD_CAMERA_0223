COMMENT comment.sh "Test if QXGA upscale is supported up to x4."

0001_SAN set -x; rszhq.sh "QXGAto8MP" && exit 0 || exit 1
0002 set -x; rszhq.sh "QXGAto5MP" && exit 0 || exit 1

