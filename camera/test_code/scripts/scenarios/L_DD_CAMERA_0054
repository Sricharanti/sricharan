COMMENT comment.sh "Test capture of a sequence of images of the supported sizes that can be save into a file using OV sensor"

0001_SAN set -x; burstest.sh "VGA" "YUYV" "2" "4" "30"&& exit 0 || exit 1
0002 set -x; burstest.sh "D1NTSC" "YUYV" "2" "4" "30"&& exit 0 || exit 1
0003_SAN set -x; burstest.sh "D1PAL" "YUYV" "2" "4" "30"&& exit 0 || exit 1
#0004 set -x; burstest.sh "SVGA" "YUYV" "2" "4" "30"&& exit 0 || exit 1
#0005_SAN set -x; burstest.sh "XGA" "YUYV" "2" "4" "30"&& exit 0 || exit 1
#0006 set -x; burstest.sh "SXGA" "YUYV" "2" "4" "15"&& exit 0 || exit 1
#0007_SAN set -x; burstest.sh "UXGA" "YUYV" "2" "4" "15"&& exit 0 || exit 1
#0008_SAN set -x; burstest.sh "QXGA" "YUYV" "2" "4" "15"&& exit 0 || exit 1

