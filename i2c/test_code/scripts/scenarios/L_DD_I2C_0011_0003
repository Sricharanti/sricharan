#######################################
##Scenario: L_DD_I2C_0011_0003
## Author: Mariia Nagul
## Date: 31 Jan 2012
## Testing: Read all phoenix addressable registers available in slave id 0x48-0x4a multiple times,
## Slaves on I2C bus 1
#######################################

# Begin L_DD_I2C_0011_0003

01 set -x; timeout=0; iterations=10 ; while [  $timeout -lt $iterations ]; do  for i in  1  2  3  4  5  6  7  8  9  a  b  c  d  e  f; do i2cdump -f -y 0x1 0x48; done; timeout=$((timeout=$timeout+1)); done

02 set -x; timeout=0; iterations=10 ; while [  $timeout -lt $iterations ]; do  for i in  1  2  3  4  5  6  7  8  9  a  b  c  d  e  f; do i2cdump -f -y 0x1 0x49; done; timeout=$((timeout=$timeout+1)); done

03 set -x; timeout=0; iterations=10 ; while [  $timeout -lt $iterations ]; do  for i in  1  2  3  4  5  6  7  8  9  a  b  c  d  e  f; do i2cdump -f -y 0x1 0x4A; done; timeout=$((timeout=$timeout+1)); done

# End L_DD_I2c_0011_0003
