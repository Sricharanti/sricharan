#!/usr/bin/expect
set timeout 10
spawn cu -l $env(CONSOLE_PORT) -s 115200 --parity=none --nostop
expect "Connected."

send "\r"
sleep 1
set t [clock clicks -millisecond]
puts start:$t
send "\r/system/xbin/su\r"
send "reboot\r"
sleep 1
expect "booting the kernel"
     set t [clock clicks -millisecond]
     puts stdout "Start:$t"
     sleep 1
     expect {
      "EXT4-fs\[ 0-9a-zA-Z():]*" {
      expect {
      "init:" {
           set t1 [clock clicks -millisecond]
           puts stdout "\rEnd:$t1"
           set r1 [expr $t1 - $t]
           puts stdout "kernel_time:$r1"
              }
         }}}
    puts stdout "FS mounted"
     send "\r"
     sleep 10
     send "~."
     expect eof

