#!/usr/bin/expect
set timeout 10
spawn cu -l $env(CONSOLE_PORT) -s 115200 --parity=none --nostop
expect "Connected."

send "\r"
sleep 1
send "\r/system/xbin/su\r"
send "reboot\r"
expect "Restarting system"
	set t [clock clicks -millisecond]
	puts start:$t

expect "Booting Linux on physical CPU 0"
     send "\r"
     sleep 1
     set t1 [clock clicks -millisecond]
     set r [expr $t1 - $t]
     puts stdout "end_time:$t1"
     puts stdout "boot_time:$r"
     send "\r"
     send "\r"
     sleep 1
     send "boot\r"
     send "\r"
     expect "booting kernel"
     sleep 10
     send "~."
     expect eof

