# Begin L_DD_OSKERNEL_0003_0001

01 set -x; if [ $(cat /sys/kernel/debug/clock/summary | grep mpu | wc -l) -gt 1 ]; then exit 0; else exit 1; fi

# End L_DD_OSKERNEL_0003_0001
