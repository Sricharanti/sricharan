#!/usr/bin/expect
set timeout 25
spawn cu -l $env(CONSOLE_PORT) -s 115200 --parity=none --nostop
expect "Connected."

send "\r"
sleep 1
send "\r/system/xbin/su\r"
send "reboot\r"
sleep 4
expect {
	"Freeing init memory" {
		puts stdout "========================="
		set t [clock clicks -millisecond]
		puts stdout "====Start:$t====="
		sleep 2
		expect {
			"*name:service.bootanim.exit" {
			set t1 [clock clicks -millisecond]
			puts stdout "Start:$t \r End:$t1"
			set r1 [expr $t1 - $t]
			puts stdout "fs_time:$r1"
			puts stdout "Ui is up"
						
				
				    }
			}
				    }
	}
     send "\r"
     sleep 3
     send "~."
     expect eof

