# Begin L_DD_AMBIENT_LIGHT_0000_0003

# Enable Ambient Light Sensor
0001 set -x; handlerSysFs.sh set $AMBIENT_LIGHT_ENABLE_POWER $AMBIENT_LIGHT_POWERON_VAL
0002 set -x; handlerSysFs.sh compare $AMBIENT_LIGHT_ENABLE_POWER $AMBIENT_LIGHT_POWERON_VAL || exit 4
 
# Disable Ambient Light Sensor
0003 set -x; handlerSysFs.sh set $AMBIENT_LIGHT_ENABLE_POWER $AMBIENT_LIGHT_POWEROFF_VAL
0004 set -x; handlerSysFs.sh compare $AMBIENT_LIGHT_ENABLE_POWER $AMBIENT_LIGHT_POWEROFF_VAL 

# End L_DD_AMBIENT_LIGHT_0000_0003
