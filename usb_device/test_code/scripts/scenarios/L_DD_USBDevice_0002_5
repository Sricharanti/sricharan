################################################################################
# Scenario: L_DD_USBDevice_0002_5
# Author  : Axel Castaneda Gonzalez
# Date    : December 4, 2006
# Description: Testing enumeration with a standard host. (Gadget File-Storage)
# ChangeLog:
#	27 Jan 2009 - Diego Zavala Trujillo - Update Scenario.
################################################################################

foo	echo L_DD_USBDevice_0002_5

0   echo -e "\n\n\t Creating a 4 Mb file to use it with the g_file_storage module. \n\n"  &&  $TESTSCRIPT/enum.sh createvFat  && exit 0 || exit 1

1   insmod  $MODULE_PATH/g_file_storage.ko file=$VFAT_FILE stall=0  &&  echo -e "\n\n\t Testing g_file_storage enumeration with a standard host. \n\t Inserting the g_file_storage module on the OMAP. \n\n"  &&  echo "0" > $LOGBASE/log.panresult  &&  exit 0  || echo -e "\n\n\t ERROR1 - No module found. \n\n"; echo "1" > $LOGBASE/log.panresult  &&  exit 1

2   if [ `cat $LOGBASE/log.panresult` = 0 ]; then export ENUM_INFO="File-backed Storage Gadget" ; ${TESTROOT}/test_runner.sh -p L_DD_USBDevice_0002_core  &&  exit 0 || exit 1; else echo -e "\n\n\t ERROR1 - Could not run this test because the module has not been inserted. \n\n"; exit 1; fi

3   echo -e "\n\n\t Delleting the 4 Mb file. \n\n"  &&  $TESTSCRIPT/enum.sh dellvFat  && exit 0 || exit 1

4   rm -rf $LOGBASE/log.panresult; rmmod g_file_storage  &&  exit 0 || exit 1

bar	echo L_DD_USBDevice_0002_5
