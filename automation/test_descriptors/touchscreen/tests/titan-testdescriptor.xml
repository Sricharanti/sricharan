<titan>

	<test id="L_DD_TOUCHSCREEN_0000_0001">

		<information>
			<autor> Abraham Arce </autor>
			<description> Touch Screen driver shall support touch screen controller </description>
			<dateofcreation> 10/06/2010 </dateofcreation>
		</information>

		<run>
			<action>
				<command>/testsuites/touchscreen/scripts/test_runner.sh L_DD_TOUCHSCREEN_0000_0001</command>
			</action>
			<action>
				<command>rm /testsuites/touchscreen/scripts/log.*</command>
			</action>
		</run>

		<pass>
			<output>
				<value>stat=0</value>
			</output>
		</pass>

		<fail>
			<timeout time="120"/>
			<output>
				<regularexpression>stat=[^0]\d*</regularexpression>
			</output>
		</fail>

	</test>

