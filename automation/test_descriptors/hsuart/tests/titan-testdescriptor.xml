				<command>/testsuites/hsuart/scripts/test_runner.sh L_DD_UART_SERIAL_0000_0001</command>
			</action>
			<action>
				<command>rm /testsuites/hsuart/scripts/log.*</command>
			</action>
		</run>

		<pass>
			<output>
				<value>stat=0</value>
			</output>
		</pass>

		<fail>
			<timeout time="600"/>
