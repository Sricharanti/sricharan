	<test id="L_DD_UART_SERIAL_0000_0001">

		<information>
			<autor> Jyothi </autor>
			<description>Test UART TX and RX with 115Kbps baudrate no flowcontrol. </description>
			<dateofcreation> 06/10/10 </dateofcreation>
		</information>

		<run>
			<action>
				<command>export PATH=$PATH:/testsuites/utils/handlers:/testsuites/utils/bin:/testsuites/hsuart/scripts/helper</command>
			</action>
			<action>
				<command>export HSUART_DIR_TMP=/testsuites/hsuart/scripts/tmp</command>
		    </action>
