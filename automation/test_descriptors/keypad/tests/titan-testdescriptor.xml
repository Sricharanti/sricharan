<titan>

	<test id="L_DD_KEYPAD_0000_0001">

		<information>
			<autor> Abraham Arce </autor>
			<description> Keypad driver shall support keyboard controller on Triton2 | Power IC </description>
			<dateofcreation> 12/5/2008 </dateofcreation>
		</information>

		<run>
			<action>
				<command>/testsuites/keypad/scripts/test_runner.sh L_DD_KEYPAD_0000_0001</command>
			</action>
			<action>
				<command>rm /testsuites/keypad/scripts/log.*</command>
			</action>
		</run>

		<pass>
			<output>
				<value>stat=0</value>
			</output>
		</pass>

		<fail>
			<timeout time="10"/>
			<output>
				<regularexpression>stat=[^0]\d*</regularexpression>
			</output>
		</fail>

	</test>

