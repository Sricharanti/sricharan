<?xml version="1.0"?>
<titan>

<test id="L_DD_RTC_0000">

 <information>
  <autor> Misael Lopez Cruz </autor>
  <description> RTC driver shall support the RTC on Triton. Only one instance is allowed at a time to access RTC device </description>
  <dateofcreation> 03/25/2008 </dateofcreation>
 </information>

 <run>
  <action>
      <command>/testsuites/realtimeclock/scripts/test_runner.sh L_DD_RTC_0000</command>
  </action>
  <action>
    <command>rm /testsuites/realtimeclock/scripts/log.*</command>
  </action>
 </run>

 <pass>
  <output>
    <value>stat=0</value>
  </output>
 </pass>

 <fail>
 <timeout time="60"/>
  <output>
    <regularexpression>stat=[^0]\d*</regularexpression>
  </output>
 </fail>

</test>

