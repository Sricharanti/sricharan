<titan>

<test id="L_DD_MMC_0001_0001"> 

 <information> 
  <autor> Misael Lopez </autor> 
  <description> Test the registration of the MMC driver </description>
  <dateofcreation> 10/20/2006 </dateofcreation> 
 </information> 

 <run>
  <action> 
      <command>export SLOT=0</command>
  </action>
  <action> 
      <command>/testsuites/mmc/scripts/test_runner.sh L_DD_MMC_0001_0001</command>
  </action>
  <action> 
    <command>rm log.*</command> 
  </action>       
 </run> 

 <pass> 
  <output> 
    <value>stat=0</value> 
  </output> 
 </pass> 

 <fail>
  <timeout time="180"/>
  <output> 
    <regularexpression>stat=[^0]\d*</regularexpression>
  </output> 
 </fail> 
  
</test> 

