<?xml version="1.0"?>
<titan>

<test id="L_DD_ONENAND_0001_0001">

 <information>
  <autor> Misael Lopez </autor>
  <description> Flash driver shall provide standard character and block interfaces to user space. Flash driver shall be registred via the CFI interface </description>
  <dateofcreation> 9/28/2006 </dateofcreation>
 </information>

 <run>
  <action>
      <command>export TYPE=OneNAND</command>
  </action>
  <action>
      <command>/testsuites/nand/scripts/test_runner.sh L_DD_ONENAND_0001_0001</command>
  </action>
  <action>
    <command>rm /testsuites/nand/scripts/log.*</command>
  </action>
 </run>

 <pass>
  <output>
    <value>stat=0</value>
  </output>
 </pass>

 <fail>
   <timeout time="300"/>
  <output>
    <regularexpression>stat=[^0]\d*</regularexpression>
  </output>
 </fail>

</test>

