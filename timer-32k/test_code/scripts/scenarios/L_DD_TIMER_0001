###################################################################################################################
# Scenario: L_DD_TIMER_0001
# Author  : Misael Lopez Cruz
# Date    : March 9th, 2007
# Description: Test the number of interrupts per second generated for the timer.
###################################################################################################################

# Begin L_DD_TIMER_0001

0001	set -x; t1=`cat /proc/interrupts | grep "$TIMER_ENTRY" | awk '{print $2}'` && t1=`echo $t1 | awk '{print $1}'` && sleep 1 && t2=`cat /proc/interrupts | grep "$TIMER_ENTRY" | awk '{print $2}'` && t2=`echo $t2 | awk '{print $1}'` && dt=`echo "$t2-$t1" | bc` && echo $dt && test $dt -ge $MIN_INT_PER_SECOND && test $dt -le $MAX_INT_PER_SECOND && exit 0 || exit 1

# End L_DD_TIMER_0001
