07  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "cat $TEXT_FILE_SAMPLE"
08  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "cat $TEXT_FILE_SAMPLE"
09  set -x; $UTILS_DIR_HANDLERS/handlerProcessParallelism.sh "add" "cat $TEXT_FILE_SAMPLE"
