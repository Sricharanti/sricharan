# Begin L_DD_UART_SERIAL_0010_0002_02

01  set -x; handlerFile.sh "create" "pattern" "$HSUART_DIR_TMP/text.file" "1000"

02  set -x; handlerProcessParallelism.sh "clean"
03  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
04  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
05  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
06  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
07  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
08  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
09  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
10  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
11  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
12  set -x; handlerProcessParallelism.sh "add" "cat $HSUART_DIR_TMP/text.file"
13  set -x; handlerProcessParallelism.sh "execute"

# End L_DD_UART_SERIAL_0010_0002_02