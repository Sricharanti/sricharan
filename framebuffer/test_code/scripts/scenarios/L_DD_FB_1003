foo   echo "L_DD_FB_1003"

COMMENT comment.sh "Verify that the FBIOBLANK ioctl can control the framebuffer."

0001 set -x; fbio.sh "blank" && exit 0 || exit 1
0002 set -x; fbio.sh "unblank" && exit 0 || exit 1

bar   echo "L_DD_FB_1003"

