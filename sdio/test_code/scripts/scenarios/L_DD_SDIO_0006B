foo 	echo "L_DD_SDIO_0006B"

0003	echo "Do you have different SDIO card types?" && sleep 1 && $UTILBIN/choice && CHOICE='Yes' || CHOICE='No' && if [ "$CHOICE" = 'Yes' ]; then (echo "Insert a different SDIO card type" && sleep 1 && $UTILBIN/waitkey && $TESTROOT/test_runner.sh -p L_DD_SDIO_0006A && exit 0 || exit 1) else (echo "Please verify this test case by inspection" && sleep 1 && $UTILBIN/akey && exit 0 || exit 1) fi

bar 	echo "L_DD_SDIO_0006B"
