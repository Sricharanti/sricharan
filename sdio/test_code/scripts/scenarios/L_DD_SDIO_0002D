foo 	echo "L_DD_SDIO_0002D"

0003	echo "Do you have a Full Speed (4-bit) SDIO card?" && sleep 1 && $UTILBIN/choice && CHOICE='Yes' || CHOICE='No' && if [ "$CHOICE" = 'Yes' ]; then (echo "Insert the Full Speed SDIO card" && sleep 1 && $UTILBIN/waitkey && $TESTROOT/test_runner.sh -p L_DD_SDIO_0002A && exit 0 || exit 1) else (echo "Please verify this test case by inspection" && sleep 1 && $UTILBIN/akey && exit 0 || exit 1) fi

bar 	echo "L_DD_SDIO_0002D"
