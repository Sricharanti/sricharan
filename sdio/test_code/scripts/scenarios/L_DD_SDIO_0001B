###################################################################################################################
# Scenario: L_DD_MMC_0001B
# Author  : Misael Lopez Cruz
# Date    : September 20, 2006
# Description: Test if SDIO device is detected when it's not present during kernel boot-up
###################################################################################################################

foo 	echo "L_DD_SDIO_0001B"

0001_a	cat $FS_LOGFILE | grep "$SDIO_TYPE detected" && exit 1 || exit 0
0001_b 	cat /proc/devices | grep 'mmc' && exit 0 || exit 1
0001_c	cat /proc/interrupts | grep "$PROC_ENTRY_DEV1" && exit 0 || exit 1

bar 	echo "L_DD_SDIO_0001B"

