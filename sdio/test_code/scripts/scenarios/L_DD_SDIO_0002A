foo 	echo "L_DD_SDIO_0002A"

0001	rm -f /sdio.msg; insmod $TESTMODS/$SDIO_MOD && cmp /sdio.msg $SDIO_MANUF_FILE && exit 0 || exit 1
0002	rm -f /sdio.msg; rmmod $SDIO_MOD && insmod $TESTMODS/$SDIO_MOD && cmp /sdio.msg $SDIO_MANUF_FILE && exit 0 || exit 1	
cleanup	rmmod $SDIO_MOD && exit 0 || exit 1
bar 	echo "L_DD_SDIO_0002A"

