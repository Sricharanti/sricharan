###############################################################################
# Scenario: L_DD_POWER_0006_0002
# Testing : Smart Reflex Auto Compensation: Enable/Disable + Suspend/Resume
###############################################################################

# Begin L_DD_POWER_0006_0002

01  set -x; suspendResume.sh  "suspend"  $WAKEUP_TIMER_DEFAULT_VALUE

02  set -x; smartReflexAutoComp.sh  enable  VDD1
03  set -x; smartReflexAutoComp.sh  enable  VDD2

04  set -x; suspendResume.sh  "suspend"  $WAKEUP_TIMER_DEFAULT_VALUE

05  set -x; smartReflexAutoComp.sh  disable  VDD1
06  set -x; smartReflexAutoComp.sh  disable  VDD2

07  set -x; suspendResume.sh  "suspend"  $WAKEUP_TIMER_DEFAULT_VALUE

# End L_DD_POWER_0006_0002