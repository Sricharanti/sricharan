###############################################################################
# Scenario: L_DD_POWER_0006_0001
# Testing : Smart Reflex Auto Compensation: Enable/Disable
###############################################################################

# Begin L_DD_POWER_0006_0001

01  set -x; debugFileSystem.sh "mount"

02  set -x; smartReflexAutoComp.sh  enable  VDD1
03  set -x; smartReflexAutoComp.sh  enable  VDD2

04  set -x; smartReflexAutoComp.sh  disable  VDD1
05  set -x; smartReflexAutoComp.sh  disable  VDD2

06  set -x; debugFileSystem.sh "umount"

# End L_DD_POWER_0006_0001
