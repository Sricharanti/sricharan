04  set -x; cpuFreqScalGovernors.sh  set  $GOVERNOR_ONDEMAND
05  set -x; cpuFreqScalGovernors.sh  set  $GOVERNOR_POWERSAVE
06  set -x; cpuFreqScalGovernors.sh  set  $GOVERNOR_USERSPACE
07  set -x; cpuFreqScalGovernors.sh  set  $GOVERNOR_PERFORMANCE

# End L_DD_POWER_0007_0001